{
  "description": "基于产品 JasOpenGIS 1.0",
  "style": "lightblue",
  "basePath": "",
  "resources": [{
      "type": "css",
      "url": "basepath:../lib/openlayer_v5.0.2/ol.css"
    },
    {
      "type": "js",
      "url": "basepath:../lib/openlayer_v5.0.2/ol.js",
      "id": "ol.js"
    },
    {
      "type": "css",
      "url": "basepath:../lib/jquery-easyui/1.5.2/themes/icon.css"
    },
    {
      "type": "css",
      "url": "basepath:../lib/jquery-easyui/1.5.2/themes/default/easyui.css"
    },
    {
      "type": "js",
      "url": "basepath:../lib/jquery/jquery-1.12.4.min.js",
      "id": "jquery.js"
    },
    {
      "type": "js",
      "url": "basepath:../lib/jquery-easyui/1.5.2/jquery.easyui.min.js",
      "relyOn": "jquery.js",
      "id": "easyui.js"
    },
    {
      "type": "js",
      "url": "basepath:../lib/jquery-easyui/1.5.2/locale/easyui-lang-zh_CN.js",
      "relyOn": "easyui.js",
      "id": "easyui-lang.js"
    },
    {
      "type": "js",
      "url": "basepath:../lib/proj4js/proj4_2.4.4.js"
    },
    {
      "type": "js",
      "url": "basepath:../lib/FileSaver.min.js"
    },

    {
      "type": "js",
      "url": "basepath:mapmodules.js"
    },{
      "type": "js",
      "url": "basepath:mapstyles/default.js",
      "relyOn": "ol.js"
    },
    {
      "type": "js",
      "url": "basepath:mapstyles/default.onepage.js",
      "relyOn": "ol.js"
    },  {
      "type": "js",
      "url": "basepath:mapstyles/mymapstyles.js",
      "relyOn": "ol.js"
    }
  ],
  "map": {
    "id": "jasMap",
    "scale": false,
    "attribution": false,
    "zoom": false,
    "mapOptions": {
      "center": [105.40071683484005,32.575070914836274],
      "level": 4,
      "projection": "EPSG:4326"
    },
    "basemaps": {
      "label": "基础底图",
      "baseMapLayers": [{
        "id": "dark_shp",
        "index": 0,
        "label": "行政区划图",
        "icon": "stylepath:images/basemap_shp.png",
        "type": "Tile",
        "source": "TileWMS",
        "url": "http://geoserver.zyax.cn/geoserver/jasgroup/wms",
        "params": {
          "LAYERS": "jasgroup:china_canton_dark"
        }
      },{
        "id": "google_img",
        "index": 1,
        "label": "谷歌影像",
        "icon": "stylepath:images/basemap_img.png",
        "type": "Tile",
        "source": "google-image"
      },{
        "id": "google_vec",
        "index": 1,
        "label": "谷歌矢量",
        "icon": "stylepath:images/basemap_img.png",
        "type": "Tile",
        "source": "google-vec"
      }]
    },
    "optionallayers": [{
      "id": "basePipeData",
      "label": "基础管线数据",
      "layerSet": [
        {
          "id": "pd_pipesegment",
          "index": 90,
          "label": "管线",
          "flash": true,
          "renderer": {
            "style": "pipesegment"
          }
        }
      ]
    }]
  },
  "proxy": [],
  "modules": []
}